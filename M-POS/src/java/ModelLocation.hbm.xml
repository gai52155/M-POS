<?xml version="1.0"?>
 
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>

    <class name="com.mpos.model.ModelLocation" table="Location">
        <id name="Name"     type="string" column="Name">
            <generator class="native"/>
        </id>
        <property name="Lat"      type="double" column="Lat"/>
        <property name="Lng"      type="double" column="Lng"/>
        <property name="ISO"      type="string" column="ISO"/>
        <property name="Province" type="string" column="Province"/>
    </class>
 
</hibernate-mapping>
