<?xml version="1.0"?>
 
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>

    <class name="com.mpos.model.ModelAbbrevilation" table="mpos_location_demo">
        <id name="abbrevilation_id" column="abbrevilation_id" type ="int">
            <generator class="native"/>
        </id>
        <property name="country" type="string" column="country"/>
        <property name="iso" type="string" column="iso"/>
    </class>
 
</hibernate-mapping>